<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_sale">

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.matty.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/navigation_sale"
        android:name="com.example.matty.ui.sale.SaleFragment"
        android:label="@string/title_sale"
        tools:layout="@layout/fragment_sale" />

    <fragment
        android:id="@+id/navigation_box"
        android:name="com.example.matty.ui.box.BoxFragment"
        android:label="@string/title_box"
        tools:layout="@layout/fragment_box" />
    <fragment
        android:id="@+id/navigation_catalogue"
        android:name="com.example.matty.ui.catalogue.CatalogueFragment"
        android:label="@string/title_catalogue"
        tools:layout="@layout/fragment_catalogue"/>
</navigation>